<?xml version="1.0" encoding="utf-8" ?>
<Patch>
 
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biome Expansion: Redwood forest</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd"> 
					<xpath>Defs/BiomeDef[defName="RedwoodForest"]/wildAnimals</xpath>
					<value>
						<ZSL_JadeSlime>0.01</ZSL_JadeSlime>
						<ZSL_WoodSlime>0.03</ZSL_WoodSlime>
						<ZSL_GoldenSlime>0.005</ZSL_GoldenSlime>
						<ZSL_RainbowSlime>0.001</ZSL_RainbowSlime>
						<ZSL_WaterSlime>0.01</ZSL_WaterSlime>
					</value>
				</li>
					
			</operations>
		</match>
	</Operation>


</Patch>

